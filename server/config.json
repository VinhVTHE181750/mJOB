{
  "boot": {
    "port": 8000,
    "maxRetries": 3,
    "retryDelay": 5000
  },
  "logger": {
    "level": "INFO"
  },

  "middleware": {
    "cors": {
      "origin": ["http://localhost:5173", "http://192.168.1.200"]
    },
    "rateLimiter": {
      "windowMs": 60000,
      "max": 10000
    }
  },

  "database": {
    "dialect": "mssql",
    "user": "remote",
    "password": "rmpass",
    "server": "localhost",
    "database": "mJob",
    "schema": "dbo",
    "encrypt": false,
    "pool": {
      "max": 8,
      "min": 0
    }
  },
  "jwt": {
    "secret": "notSoSecret",
    "expiresIn": 86400,
    "issuer": "mJob",
    "algorithm": "HS256",
    "refreshSecret": "notFreshSecret",
    "refreshExpiresIn": 2592000
  },
  "sessionSecret": "notSoSecret"
}
